    "@types/three": "^0.159.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0",
    "globals": "^15.0.0",
